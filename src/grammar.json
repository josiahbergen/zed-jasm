{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "jasm",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "instruction"
                },
                {
                  "type": "SYMBOL",
                  "name": "label"
                },
                {
                  "type": "SYMBOL",
                  "name": "macro"
                },
                {
                  "type": "SYMBOL",
                  "name": "data"
                },
                {
                  "type": "SYMBOL",
                  "name": "macro_call"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "MACRO"
        },
        {
          "type": "SYMBOL",
          "name": "label_name"
        },
        {
          "type": "SYMBOL",
          "name": "macro_args"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "macro_line"
          }
        },
        {
          "type": "STRING",
          "value": "END MACRO"
        }
      ]
    },
    "macro_args": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "macro_arg"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "macro_arg"
              }
            ]
          }
        }
      ]
    },
    "macro_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "instruction"
                },
                {
                  "type": "SYMBOL",
                  "name": "data"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "macro_arg": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "SYMBOL",
          "name": "label_name"
        }
      ]
    },
    "macro_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "op_list"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "instruction": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "mnemonic"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "op_list"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "op_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_generic_op"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_generic_op"
              }
            ]
          }
        }
      ]
    },
    "_generic_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "operand"
        },
        {
          "type": "SYMBOL",
          "name": "macro_arg"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "operand": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "register_pair"
        },
        {
          "type": "SYMBOL",
          "name": "register"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "label_name"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "operator"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_exp_term"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "operator"
              },
              {
                "type": "SYMBOL",
                "name": "_exp_term"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_exp_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "macro_arg"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "STRING",
          "value": "<<"
        },
        {
          "type": "STRING",
          "value": ">>"
        },
        {
          "type": "STRING",
          "value": "&"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "STRING",
          "value": "^"
        },
        {
          "type": "STRING",
          "value": "~"
        }
      ]
    },
    "data": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "DATA"
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "constant"
              }
            ]
          }
        }
      ]
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label_name"
        },
        {
          "type": "STRING",
          "value": ":"
        }
      ]
    },
    "mnemonic": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[lL][oO][aA][dD]"
        },
        {
          "type": "PATTERN",
          "value": "[sS][tT][oO][rR][eE]"
        },
        {
          "type": "PATTERN",
          "value": "[mM][oO][vV][eE]"
        },
        {
          "type": "PATTERN",
          "value": "[pP][uU][sS][hH]"
        },
        {
          "type": "PATTERN",
          "value": "[pP][oO][pP]"
        },
        {
          "type": "PATTERN",
          "value": "[aA][dD][dD]"
        },
        {
          "type": "PATTERN",
          "value": "[aA][dD][dD][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[sS][uU][bB]"
        },
        {
          "type": "PATTERN",
          "value": "[sS][uU][bB][bB]"
        },
        {
          "type": "PATTERN",
          "value": "[iI][nN][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[dD][eE][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[lL][sS][hH][fF]"
        },
        {
          "type": "PATTERN",
          "value": "[rR][sS][hH][fF]"
        },
        {
          "type": "PATTERN",
          "value": "[aA][nN][dD]"
        },
        {
          "type": "PATTERN",
          "value": "[oO][rR]"
        },
        {
          "type": "PATTERN",
          "value": "[nN][oO][rR]"
        },
        {
          "type": "PATTERN",
          "value": "[nN][oO][tT]"
        },
        {
          "type": "PATTERN",
          "value": "[xX][oO][rR]"
        },
        {
          "type": "PATTERN",
          "value": "[iI][nN][bB]"
        },
        {
          "type": "PATTERN",
          "value": "[oO][uU][tT][bB]"
        },
        {
          "type": "PATTERN",
          "value": "[cC][mM][pP]"
        },
        {
          "type": "PATTERN",
          "value": "[sS][eE][tT][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[cC][lL][rR][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[cC][lL][rR][zZ]"
        },
        {
          "type": "PATTERN",
          "value": "[jJ][uU][mM][pP]"
        },
        {
          "type": "PATTERN",
          "value": "[jJ][zZ]"
        },
        {
          "type": "PATTERN",
          "value": "[jJ][nN][zZ]"
        },
        {
          "type": "PATTERN",
          "value": "[jJ][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[jJ][nN][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[iI][nN][tT]"
        },
        {
          "type": "PATTERN",
          "value": "[hH][aA][lL][tT]"
        },
        {
          "type": "PATTERN",
          "value": "[nN][oO][pP]"
        }
      ]
    },
    "register_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "register"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "register"
        }
      ]
    },
    "register": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[aA]"
        },
        {
          "type": "PATTERN",
          "value": "[bB]"
        },
        {
          "type": "PATTERN",
          "value": "[cC]"
        },
        {
          "type": "PATTERN",
          "value": "[dD]"
        },
        {
          "type": "PATTERN",
          "value": "[xX]"
        },
        {
          "type": "PATTERN",
          "value": "[yY]"
        },
        {
          "type": "PATTERN",
          "value": "[sS][pP]"
        },
        {
          "type": "PATTERN",
          "value": "[pP][cC]"
        },
        {
          "type": "PATTERN",
          "value": "[zZ]"
        },
        {
          "type": "PATTERN",
          "value": "[fF]"
        },
        {
          "type": "PATTERN",
          "value": "[mM][bB]"
        },
        {
          "type": "PATTERN",
          "value": "[sS][tT][sS]"
        }
      ]
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "0[xX][0-9a-fA-F]+"
        },
        {
          "type": "PATTERN",
          "value": "0[bB][01]+"
        },
        {
          "type": "PATTERN",
          "value": "[0-9]+"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "label_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_]*"
    },
    "comment": {
      "type": "PATTERN",
      "value": ";.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
